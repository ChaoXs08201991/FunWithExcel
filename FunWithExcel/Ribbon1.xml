<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="chaoxs" label="QuickTools">
        <group id="AutoGenerate"
               label="自动生成">
          <button id="Button1"
                  label="立柱轴线偏差生成"
                  size="normal"
                  imageMso="AutoFormatDialog"
                  screentip="使用说明"
                  supertip="先将相关数据Copy到本Sheet再进行操作"
                  onAction="onAutoGen"/>
        </group >
        <group id="AutoMonitor"
               label="自动化监测报表">
          <button id="Button3"
                  label="添加棱镜常数"
                  size="normal"
                  imageMso="AddInsMenu"
                  onAction="onCalPrism"/>
          <button id="Button4"
                  label="计算控制点坐标"
                  size="normal"
                  imageMso="CalculateSheet"
                  onAction="onCalBase"/>
          <button id="Button5"
                  label="替换报表"
                  size="normal"
                  imageMso="MailMergeMatchFields"
                  onAction="onReplace"/>
          <button id="Button6"
                  label="生成报表"
                  size="normal"
                  imageMso="PivotTableGenerateGetPivotData"
                  onAction="onGenerate"/>
        </group>
        <group id ="ThreeDLaserScan"
               label="APM计算">
          <button id="Button7"
                  label="生成APM计算表"
                  size="normal"
                  imageMso="MoviePlayAutomatically"
                  onAction="onCalAPM"/>
          <button id="Button8"
                  label="获取SCENE文件序号"
                  size="normal"
                  imageMso="WordOpenParaAbove"
                  onAction="onCalAPM1"/>
          <button id="Button9"
                  label="APM计算"
                  size="normal"
                  imageMso="Calculator"
                  onAction="onCalAPM2"/>
        </group>
        <group id="About"
               label="关于">
          <button id="Button2"
                  size="large"
                  imageMso="TentativeAcceptInvitation"
                  screentip="About"
                  supertip="Excel插件 by VSTO VB.Net"
                  onAction="onAbout1" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>