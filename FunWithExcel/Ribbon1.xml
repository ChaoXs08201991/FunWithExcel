<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab id="chaoxs" label="FunWithExcel">
        <group id="AutoGenerate"
               label="自动生成">
          <button id="Button1"
                  label="立柱轴线偏差生成"
                  size="normal"
                  imageMso="AutoFormatDialog"
                  onAction="onAutoGen" />
          <button id="Button2"
                  label="关于"
                  size="normal"
                  imageMso="AnimationTransitionGallery"
                  screentip="使用说明"
                  supertip="鼠标放在上面显示使用说明"
                 onAction="onAbout1" />
        </group >
        <group id="AutoMonitor"
               label="自动化监测报表">
          <button id="Button3"
                  label="添加棱镜常数"
                  size="normal"
                  imageMso="AddInsMenu"
                  onAction="onCalPrism"/>
        <button id="Button4"
                  label="计算控制点坐标"
                  size="normal"
                  imageMso="Calculator"
                  onAction="onCalBase"/>
        <button id="Button5"
                  label="替换报表"
                  size="normal"
                  imageMso="ReplaceDialog"
                  onAction="onReplace"/>
        <button id="Button6"
                  label="生成报表"
                  size="normal"
                  imageMso="PivotTableGenerateGetPivotData"
                  onAction="onGenerate"/>
        </group>         
      </tab>
    </tabs>
  </ribbon>
</customUI>